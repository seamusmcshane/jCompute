<?xml version="1.0" encoding="ISO-8859-1"?>
<Scenario xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas/lv-schema.xmls">
	<Header>
		<Version>0.01</Version>
		<Type>LV</Type>
	</Header>
	<Statistics>
		<Stat>
			<Name>Population</Name>
			<Enabled>true</Enabled>
			<TotalStat>true</TotalStat>
			<Graph>true</Graph>
			<GraphSampleRate>64</GraphSampleRate>
		</Stat>
	</Statistics>
	<Integration>
		<Method>RK4</Method>
		<SubSteps>64</SubSteps>
	</Integration>
	<Parameters>
		<Parameter>
			<Name>initial_prey_population</Name>
			<Value>800</Value>
		</Parameter>
		<Parameter>
			<Name>initial_predator_population</Name>
			<Value>100</Value>
		</Parameter>
		<Parameter>
			<Name>prey_growth</Name>
			<Value>0.5</Value>
		</Parameter>
		<Parameter>
			<Name>predation_rate</Name>
			<Value>0.008</Value>
		</Parameter>
		<Parameter>
			<Name>predator_death_rate</Name>
			<Value>0.8</Value>
		</Parameter>
		<Parameter>
			<Name>predator_conversion_rate</Name>
			<Value>0.2</Value>
		</Parameter>
	</Parameters>
</Scenario>
