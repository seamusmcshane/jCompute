<Batch xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas/batch.xsd">
	<Header>
		<Version>0.01</Version>
		<Type>Batch</Type>
	</Header>
	<Config>
		<BatchDescription>Predator/Prey with intiial numbers from 100 to 1000</BatchDescription>
		<BaseScenarioFileName>BatchBenchmarkScenario.xml</BaseScenarioFileName>
		<BatchStatsExportDir>stats/</BatchStatsExportDir>
		<BatchDirName>IntialNumbers-(Predator-Prey-100-1000)</BatchDirName>
		<ItemSamples>10</ItemSamples>
	</Config>
	<Parameters>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Predator</GroupName>
			<ParameterName>InitialNumbers</ParameterName>
			<Intial>100</Intial>
			<Increment>100</Increment>
			<Combinations>10</Combinations>
		</Parameter>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Prey</GroupName>
			<ParameterName>InitialNumbers</ParameterName>
			<Intial>100</Intial>
			<Increment>100</Increment>
			<Combinations>10</Combinations>
		</Parameter>
	</Parameters>
</Batch>
