<Batch xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas/batch.xsd">
	<Header>
		<Version>0.02</Version>
		<Type>Batch</Type>
	</Header>
	<Config>
		<BaseScenarioFileName>400x800x3000.scenario</BaseScenarioFileName>
		<ItemSamples>100</ItemSamples>
	</Config>
	<Log>
		<InfoLog>true</InfoLog>
		<ItemLog>true</ItemLog>
	</Log>
	<Stats>
		<Store>true</Store>
		<SingleArchive>true</SingleArchive>
		<BufferSize>65536</BufferSize>
		<CompressionLevel>1</CompressionLevel>
		<BatchStatsExportDir>stats/</BatchStatsExportDir>
		<BatchGroupDir>Sensitivity</BatchGroupDir>
		<BatchSubGroupDirName>Size</BatchSubGroupDirName>
	</Stats>
	<Parameters>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Predator</GroupName>
			<ParameterName>Size</ParameterName>
			<Intial>0</Intial>
			<Increment>0.1</Increment>
			<Combinations>100</Combinations>
		</Parameter>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Prey</GroupName>
			<ParameterName>Size</ParameterName>
			<Intial>0</Intial>
			<Increment>0.1</Increment>
			<Combinations>100</Combinations>
		</Parameter>
	</Parameters>
</Batch>
