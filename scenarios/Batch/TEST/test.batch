<Batch xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas/batch.xsd">
	<Header>
		<Version>0.01</Version>
		<Type>Batch</Type>
	</Header>
	<Config>
		<BaseScenarioFileName>test.scenario</BaseScenarioFileName>
		<ItemSamples>1</ItemSamples>
	</Config>
	<Log>
		<InfoLog>true</InfoLog>
		<ItemLog>true</ItemLog>
	</Log>
	<Stats>
		<Store>false</Store>
		<BatchStatsExportDir>stats/</BatchStatsExportDir>
		<BatchGroupDir>TEST-Batch</BatchGroupDir>
	</Stats>
	<Parameters>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Predator</GroupName>
			<ParameterName>ViewRange</ParameterName>
			<Intial>0</Intial>
			<Increment>5</Increment>
			<Combinations>10</Combinations>
		</Parameter>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Prey</GroupName>
			<ParameterName>ViewRange</ParameterName>
			<Intial>0</Intial>
			<Increment>5</Increment>
			<Combinations>10</Combinations>
		</Parameter>
	</Parameters>
</Batch>
