<Batch xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas/batch.xsd">
	<Header>
		<Version>0.01</Version>
		<Type>Batch</Type>
	</Header>
	<Config>
		<BatchDescription>
		Predator-Prey scenario with evenly matched species, hunger enabled - varing view range.
		</BatchDescription>
		<BaseScenarioFileName>scenario-even.xml</BaseScenarioFileName>
		<BatchStatsExportDir>stats/</BatchStatsExportDir>
		<BatchDirName>PP(2S)-VR(5-100)-HT(100)-sam(10)-even-speed</BatchDirName>
		<ItemSamples>10</ItemSamples>
	</Config>
	<Parameters>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Predator</GroupName>
			<ParameterName>ViewRange</ParameterName>
			<Intial>5</Intial>
			<Increment>5</Increment>
			<Combinations>20</Combinations>
		</Parameter>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Prey</GroupName>
			<ParameterName>ViewRange</ParameterName>
			<Intial>5</Intial>
			<Increment>5</Increment>
			<Combinations>20</Combinations>
		</Parameter>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Predator</GroupName>
			<ParameterName>HungerThreshold</ParameterName>
			<Intial>100</Intial>
			<Increment>0</Increment>
			<Combinations>1</Combinations>
		</Parameter>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Prey</GroupName>
			<ParameterName>HungerThreshold</ParameterName>
			<Intial>100</Intial>
			<Increment>0</Increment>
			<Combinations>1</Combinations>
		</Parameter>
	</Parameters>
</Batch>
