<Batch xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas/batch.xsd">
	<Header>
		<Version>0.01</Version>
		<Type>Batch</Type>
	</Header>
	<Config>
		<BatchDescription>512 Item IO Test Batch</BatchDescription>
		<BaseScenarioFileName>scenario.scenario</BaseScenarioFileName>
		<BatchStatsExportDir>stats/</BatchStatsExportDir>
		<BatchDirName>512IOTEST</BatchDirName>
		<ItemSamples>8</ItemSamples>
	</Config>
	<Parameters>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Predator</GroupName>
			<ParameterName>InitialNumbers</ParameterName>
			<Intial>250</Intial>
			<Increment>250</Increment>
			<Combinations>8</Combinations>
		</Parameter>
		<Parameter>
			<Type>Group</Type>
			<Path>Agents.SimpleAgent</Path>
			<GroupName>Prey</GroupName>
			<ParameterName>InitialNumbers</ParameterName>
			<Intial>250</Intial>
			<Increment>250</Increment>
			<Combinations>8</Combinations>
		</Parameter>
	</Parameters>
</Batch>
